from itertools import combinations_with_replacement,permutations
n=int(input())
l2=[]
for i in range(1,n+1):
    l=combinations_with_replacement([1,2],i)
    l1=permutations([1,2],i)
    #print(list(l1))
    for i in list(l1):
        if sum(i)==n:
            if i not in l2:
                l2.append(i)
    for i in list(l):
        if sum(i)==n:
            if i not in l2:star
                l2.append(i)
print(len(l2),l2)
